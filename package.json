{
  "name": "@duckarchive/map",
  "version": "0.1.0",
  "description": "Interactive historical map component for visualizing geographical data over time",
  "keywords": [
    "react",
    "leaflet",
    "map",
    "historical",
    "geojson",
    "ukraine",
    "geography"
  ],
  "homepage": "https://github.com/duckarchive/map.duckarchive.com#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duckarchive/map.duckarchive.com.git"
  },
  "bugs": {
    "url": "https://github.com/duckarchive/map.duckarchive.com/issues"
  },
  "license": "MIT",
  "author": "DuckArchive",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "geojson"
  ],
  "scripts": {
    "fetch-ohm-borders": "tsx scripts/fetchAllOHMBorders.ts",
    "filter-by-ua-borders": "tsx scripts/filterByUABorders.ts",
    "rename-feature-props": "tsx scripts/renameFeatureProps.ts",
    "extract-translations": "tsx scripts/extractForTranslations.ts",
    "apply-translations": "tsx scripts/applyTranslations.ts",
    "topo-to-geo": "tsx scripts/convertTopoToGeo.ts",
    "generate-ids": "tsx scripts/generateIds.ts",
    "fix": "tsx scripts/fix.ts",
    "gisloader": "tsx scripts/gisloader.ts",
    "merge": "tsx scripts/merge.ts",
    "minify": "tsx scripts/minify.ts",
    "translator": "tsx scripts/translator.ts",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:lib": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist && mv dist/src/* dist/ && rm -rf dist/src",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "cd demo && vite",
    "build:demo": "cd demo && vite build",
    "preview": "cd demo && vite preview",
    "prepare-package": "cp README.package.md README.md",
    "prepublishOnly": "npm run prepare-package && npm run build:lib",
    "postpublish": "git checkout README.md",
    "pack-test": "npm run prepare-package && npm run build:lib && npm pack",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release": "npm run build:lib && npm run pack-test",
    "publish:github": "npm run prepare-package && npm run build:lib && npm publish"
  },
  "peerDependencies": {
    "leaflet": "^1.9.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-leaflet": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@heroui/autocomplete": {
      "optional": true
    },
    "@heroui/button": {
      "optional": true
    },
    "@heroui/card": {
      "optional": true
    },
    "@heroui/input": {
      "optional": true
    },
    "@heroui/spinner": {
      "optional": true
    },
    "@heroui/system": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    },
    "swr": {
      "optional": true
    }
  },
  "dependencies": {
    "@turf/area": "^7.2.0",
    "@turf/bbox-polygon": "^7.2.0",
    "@turf/boolean-contains": "^7.2.0",
    "@turf/boolean-intersects": "^7.2.0",
    "@turf/boolean-overlap": "^7.2.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/buffer": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/inside": "^5.0.0",
    "@turf/intersect": "^7.2.0",
    "@turf/random": "^7.2.0",
    "@turf/simplify": "^7.2.0",
    "json5": "^2.2.3",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet-geosearch": "^4.2.0",
    "proj4": "^2.9.0"
  },
  "devDependencies": {
    "@heroui/autocomplete": "^2.3.26",
    "@heroui/button": "^2.2.24",
    "@heroui/card": "^2.2.23",
    "@heroui/input": "^2.4.25",
    "@heroui/spinner": "^2.2.21",
    "@heroui/system": "^2.4.20",
    "@tailwindcss/vite": "4.1.12",
    "@turf/union": "^7.2.0",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.20",
    "@types/lodash": "^4.17.20",
    "@types/proj4": "^2.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/topojson-client": "^3.1.5",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "csv-parse": "^5.5.0",
    "csv-stringify": "^6.6.0",
    "geojson": "^0.5.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "swr": "^2.3.6",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "topojson-client": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
